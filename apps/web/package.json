{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "start:with-env": "pnpm with-env pnpm start",
    "with-env": "dotenv -e ./.env --",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "react-router:typegen": "react-router typegen",
    "supabase": "supabase",
    "supabase:start": "supabase status || supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:test": "supabase db test",
    "supabase:db:lint": "supabase db lint",
    "supabase:deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "supabase:typegen": "pnpm run supabase:typegen:packages && pnpm run supabase:typegen:app",
    "supabase:typegen:packages": "supabase gen types typescript --local > ../../packages/supabase/src/database.types.ts",
    "supabase:typegen:app": "supabase gen types typescript --local > ./lib/database.types.ts",
    "supabase:db:dump:local": "supabase db dump --local --data-only"
  },
  "dependencies": {
    "@edge-csrf/core": "2.5.3-cloudflare-rc1",
    "@hookform/resolvers": "^4.1.3",
    "@kit/accounts": "workspace:*",
    "@kit/admin": "workspace:*",
    "@kit/analytics": "workspace:*",
    "@kit/auth": "workspace:*",
    "@kit/billing": "workspace:*",
    "@kit/billing-gateway": "workspace:*",
    "@kit/cms": "workspace:*",
    "@kit/csrf": "workspace:*",
    "@kit/database-webhooks": "workspace:*",
    "@kit/email-templates": "workspace:*",
    "@kit/i18n": "workspace:*",
    "@kit/mailers": "workspace:*",
    "@kit/monitoring": "workspace:*",
    "@kit/notifications": "workspace:*",
    "@kit/otp": "workspace:*",
    "@kit/shared": "workspace:*",
    "@kit/supabase": "workspace:*",
    "@kit/team-accounts": "workspace:*",
    "@kit/ui": "workspace:*",
    "@marsidev/react-turnstile": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@react-router/fs-routes": "7.4.0",
    "@react-router/node": "^7.4.0",
    "@react-router/serve": "^7.4.0",
    "@supabase/supabase-js": "^2.49.3",
    "@tanstack/react-query": "5.69.0",
    "@tanstack/react-table": "^8.21.2",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.25",
    "lucide-react": "^0.484.0",
    "next-themes": "0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-router": "^7.4.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@kit/eslint-config": "workspace:*",
    "@kit/prettier-config": "workspace:*",
    "@kit/tailwind-config": "workspace:*",
    "@kit/tsconfig": "workspace:*",
    "@react-router/dev": "^7.4.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.2.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "supabase": "2.20.5",
    "tailwindcss": "4.0.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
